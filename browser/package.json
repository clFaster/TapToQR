{
  "name": "tap-to-qr",
  "version": "0.0.0",
  "description": "Turn any page into a QR code in seconds. TapToQR is a lightweight browser extension for sharing pages and custom content.",
  "license": "GPL-3.0",
  "author": "Moritz Reis",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc",
  "scripts": {
    "dev": "vite build --watch",
    "start:firefox": "concurrently -k -n BUILD,FIREFOX -c cyan,magenta \"pnpm dev\" \"pnpm start:firefox:browser\"",
    "start:firefox:browser": "web-ext run --start-url https://mozilla.org --browser-console --source-dir build",
    "start:edge": "concurrently -k -n BUILD,EDGE -c cyan,green \"pnpm dev\" \"pnpm start:edge:browser\"",
    "start:edge:browser": "web-ext run --target chromium --chromium-binary \"C:\\Program Files (x86)\\Microsoft\\Edge\\Application\\msedge.exe\" --start-url https://www.microsoft.com --source-dir build",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "catalog:",
    "@fortawesome/free-regular-svg-icons": "^7.2.0",
    "@fortawesome/free-solid-svg-icons": "catalog:",
    "@fortawesome/react-fontawesome": "catalog:",
    "jimp": "^1.6.0",
    "qrcode": "^1.5.4",
    "react": "catalog:",
    "react-dom": "catalog:",
    "styled-components": "^6.3.9",
    "vite-plugin-static-copy": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/qrcode": "^1.5.6",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/webextension-polyfill": "^0.12.4",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "catalog:",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.3.0",
    "typescript": "catalog:",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1",
    "concurrently": "^9.2.1",
    "web-ext": "^9.3.0",
    "webextension-polyfill": "^0.12.0"
  }
}
